{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "allowJs": true,
      "declaration": true,
      "outDir": "./dist",
      "strict": false,
      "esModuleInterop": true,
      "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "ES2016.Array.Include"],
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
    }
  }